class Solution {
    public String removeVowels(String s) {
        StringBuilder sb = new StringBuilder();
        for (char ch : s.toCharArray()) {
            if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u')
                continue;
            else
                sb.append(ch);
        }
        return sb.toString();
    }
}
